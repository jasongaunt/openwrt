&tlmm {
	serial0_pins: serial0_pinmux {
		mux {
			pins = "gpio16", "gpio17";
			function = "blsp_uart0";
			bias-disable;
		};
	};
	serial1_pins: serial1_pinmux {
		mux {
			pins = "gpio8", "gpio9";
			function = "blsp_uart1";
			bias-disable;
		};
	};
	uart1_pins: uart1_pinmux {
		mux {
			pins = "gpio8", "gpio9", "gpio10", "gpio11";
			function = "blsp_uart1";
			bias-disable;
		};
	};
	spi_0_pins: spi_0_pinmux {
		mux {
			function = "blsp_spi0";
			pins = "gpio13", "gpio14", "gpio15";
			// drive-strength = <12>;
			bias-disable;
		};

		mux_cs {
			function = "gpio";
			pins = "gpio12";
			// drive-strength = <2>;
			bias-disable;
			output-low;
		};
	};
	i2c_0_pins: i2c_0_pinmux {
		mux {
			bias-disable;
			function = "blsp_i2c0";
			pins = "gpio58", "gpio59";
		};
	};
	// led_pins: led_pinmux {
	// 	mux {
	// 		pins = "gpio44", "gpio45", "gpio46";
	// 		drive-strength = <0x8>;
	// 		bias-pull-up;
	// 		output-high;
	// 	};
	// };
};

&blsp1_spi1 { /* BLSP1 QUP1 */
	pinctrl-0 = <&spi_0_pins>;
	pinctrl-names = "default";
	status = "okay";
	cs-gpios = <&tlmm 54 GPIO_ACTIVE_HIGH>;

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <50000000>;
		status = "okay";
		m25p,fast-read;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition0@0 {
				label = "0:QSEE";
				reg = <0x00000000 0x00060000>;
				read-only;
			};
			partition1@60000 {
				label = "u-boot";
				reg = <0x00060000 0x00080000>;
				read-only;
			};
			partition2@E0000 {
				label = "env";
				reg = <0x000E0000 0x00010000>;
				read-only;
			};
			partition3@F0000 {
				label = "0:ART";
				reg = <0x000F0000 0x00010000>;
				read-only;
			};
			partition4@100000 {
				label = "dualflag";
				reg = <0x00100000 0x00010000>;
			};
			partition5@110000 {
				label = "CRT";
				reg = <0x00110000 0x00010000>;
				read-only;
			};
			partition6@120000 {
				label = "reserved";
				reg = <0x00120000 0x00260000>;
			};
		};
	};
};

&blsp1_uart1 {
	pinctrl-0 = <&serial1_pins>;
	pinctrl-names = "default";
	status = "okay";
};

